{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "Android >= 4",
            "iOS >= 7"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "transform-runtime",
    [
      "component",
      [
        {
          "libraryName": "fangapp-pop",
          "style": true
        }
      ]
    ]
  ]
}